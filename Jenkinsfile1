pipeline {
    options {
        timeout(time: 10, unit: 'MINUTES')
    }
    agent {
        label 'master'
    }
    stages {
        stage('Checkout') {
            steps {
                script {
                    dir('release') {
                        deleteDir()
                        checkout([$class: 'GitSCM', branches: [[name: '*/vered']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: 'DevOpsINT', url: 'https://github.com/DevOpsINT/release.git']]])    
                    }
                    print("BuildVersion is production ")
                }
            }
       }
   }
}   
   
