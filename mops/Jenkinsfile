def ls_command
pipeline {
    agent {
        label 'master'
    }
    stages {
        stage('checkout'){
            steps {
                script {
                    git credentialsId: 'devopint', url: 'https://github.com/DevOpsINT/Course.git'
                }
            }
        }
        stage('shell command example') {
            steps {
                git branch: 'master',
                    credentialsId: '95f577d1-e5ee-47ea-b766-967b233d7bf4',
                    url: 'https://github.com/DevOpsINT/Course.git'
                
                sh 'git branch' 
            }
        }
    }
}
