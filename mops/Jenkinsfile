def ls_command
pipeline {
    agent {
        label 'master'
    }
    stages {
        stage('checkout'){
            steps {
                script {
                    git credentialsId: 'devopint', url: 'https://github.com/DevOpsINT/Course.git'
                }
            }
        }
        stage('shell command example') {
            steps {
                
           git(
       url: 'https://github.com/DevOpsINT/Course/',
       credentialsId: 'mopsGit?Course',
       branch: "${mops}"
               
               sh 'git branch'
    )
                
                
            }
        }
    }
}
