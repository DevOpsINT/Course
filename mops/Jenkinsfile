def ls_command
pipeline {
    agent {
        label 'master'
    }
    stages {
        stage('checkout'){
            steps {
                script {
                    git credentialsId: 'devopint', url: 'https://github.com/DevOpsINT/Course.git'
                }
            }
        }
        stage('shell command example') {
            steps {
                sh 'touch john'
                sh 'git config --global user.email "relevant@example.com"'
                sh 'git config --global user.name "DevOpsINT"'
                sh 'git checkout mops'
                sh 'git add .'
                sh 'git commit -m "new file"'
                sh 'git push origin mops'
                
            }
        }
    }
}
